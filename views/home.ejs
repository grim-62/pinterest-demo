  <div id="main">
  
    <div id="top-bar" class="fixed-top bg-white pt-2">
      <div class="menu">
        <img draggable="false" class="logo" style="width: 5vh;" src="https://cdn1.iconfinder.com/data/icons/logotypes/32/pinterest-512.png">
        <a class="home" >Home</a>
        <h5><a class="text-black p-3 text-decoration-none" ><b>Explore</b></a></h5>
        <h5><a class="text-black p-3 text-decoration-none" ><b>Create</b></a></h5>
        <input id="one" type="text" class="search-bar" placeholder="Search">
        <button id="two" class="search-bar"></button>
        <div class="icon-container">
          <img tabindex="1" draggable="false" class="icon bell" style="width: 4.5vh;" src="https://i.hizliresim.com/rcvolcu.png">
          <img tabindex="1" draggable="false" class="icon message" style="width: 4.5vh;" src="https://i.hizliresim.com/newumah.png">
          <img tabindex="1" draggable="false" class="icon pp" style="width: 4.5vh;" src="https://i.pinimg.com/564x/bd/94/ce/bd94ce28cf8aefb521bac31d547f6409.jpg">
          <img tabindex="1" draggable="false" class="icon btn" src="https://i.hizliresim.com/g15mqud.png">
          
          <!----FOCUS----->
          <div tabindex="1" class="focus-bar bell-bar">nothing</div>
          <!-------------------->
          <div tabindex="1" class="focus-bar message-bar">you are alone</div>
          <!-------------------->
          <div tabindex="1" class="focus-bar pp-bar"><img src="https://i.pinimg.com/564x/bd/94/ce/bd94ce28cf8aefb521bac31d547f6409.jpg"><a href="/profile">Profile</a></div>
